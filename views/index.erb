<p>
  Below is a visualization of the <em>Sigma</em> lenses from DWI
</p>

<div id="graph"/>

<script>
  $(function () {

    lenses = <%= lenses_json %>;

    data = []
    $.each(lenses, function(index, value) {
      data.push({
        data: value['plot'],
        lines: { show: true },
        points: { show: true }
      });
    });

    $.plot($("#graph"), data);
  });
</script>
